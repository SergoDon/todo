{
    "env": {
        "browser": true,
        "es6": true
    },
    
    "extends": [
        "react-app",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "all",
                "tabWidth": 2,
                "semi": true,
                "singleQuote": false,
                "printWidth": 120,
                "useTabs": false
            }
        ]
    }

}